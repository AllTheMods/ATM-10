	quest.06EAA74E0A10CBB6.quest_subtitle: "Rango: &b36&r Bloques"
	quest.06EAA74E0A10CBB6.title: "&9Acelerador de Crecimiento de Imperium"
	quest.06F2C42E6408149E.quest_desc: [
		"¡Espero que no le hayas robado esto al &c&lPiromante&r, él es el bueno!"
		"{image:atm:textures/questpics/iron_spells/spells_outfit_pyromancer.png width:50 height:100 align:center}"
