	quest.2114BABF547A0E2A.title: "Toast"
	quest.211FD55DEB635A98.quest_desc: ["{image:atm:textures/questpics/basicarmor/armor_copper.png width:100 height:150 align:center}"]
	quest.211FD55DEB635A98.title: "&6Медь&r"
	quest.21234BB40DB05C78.title: "Литейная форма: Браслет"
	quest.212EC1F41227184D.quest_desc: ["Хотите стрелять эндер-жемчугом, как &eЛич&r? Тогда &9скипетр сумерек&r — это ваш выбор!\\n\\nДля перезарядки соедините его с &5эндер-жемчугом&r в верстаке."]
