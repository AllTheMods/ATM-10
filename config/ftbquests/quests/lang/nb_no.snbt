	quest.4BACAC18E4B982DA.title: "Cinnabar Vein"
	quest.4BDE773C3359D584.quest_subtitle: "A Mini-Robot To Accompany You On Your Travels"
	quest.4BF0BB763BFFACF0.quest_desc: [
